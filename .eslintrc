{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "no-undef": 0,
    "no-var": 1,
    "prefer-arrow-callback": 1,
    "prefer-const": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "no-constant-condition": 0,
    "no-case-declarations": 0
  },
  "plugins": [
    "babel"
  ]
}
